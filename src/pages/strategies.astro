---
// Import components
import Layout from '../layouts/Layout.astro';

// Import MDX content
import StrategiesContent from '../content/strategies.mdx';

// SEO metadata
const title = "AI Performance Optimization Strategies: Expert Techniques & Best Practices";
const description = "Master AI performance optimization with proven strategies for cost reduction, performance tuning, and infrastructure optimization. Expert techniques for maximizing AI ROI.";
const canonicalURL = "https://aiperformanceoptimization.com/strategies";
---

<Layout 
  title={title} 
  description={description} 
  canonicalURL={canonicalURL}
  currentPage="strategies"
>
  <section class="page-header">
    <div class="container">
      <h1>AI Performance Optimization Strategies</h1>
      <p>Proven techniques and best practices for optimizing AI performance, reducing costs, and maximizing ROI</p>
    </div>
  </section>

  <main class="container">
    <StrategiesContent />
  </main>
</Layout> 